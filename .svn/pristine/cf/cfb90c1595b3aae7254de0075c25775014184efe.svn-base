#按手机号查询
sql_queryVehicle=SELECT V.VID AS VID,V.VEHICLE_NO AS VEHICLE_NO,S.COMMADDR AS COMMADDR,T.TMAC AS TMAC,T.OEM_CODE AS OEM_CODE FROM TB_VEHICLE V ,TB_SIM S ,TB_TERMINAL T ,TR_SERVICEUNIT TR WHERE TR.VID=V.VID AND TR.SID=S.SID AND TR.TID=T.TID AND V.ENABLE_FLAG=1 AND S.ENABLE_FLAG=1 AND T.ENABLE_FLAG=1 AND S.COMMADDR >= ? AND S.COMMADDR <= ? AND ROWNUM <=500000
